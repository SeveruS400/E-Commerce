@model IEnumerable<Categories>
<div>
    <ul class="list-group">
        @foreach (var category in Model)
        {
            <li class="list-group-item">
                <a asp-action="Index" asp-route-categoryId="@category.Id">
                    @category.CategoryName
                </a>
                
            </li>
        }
    </ul>
</div>